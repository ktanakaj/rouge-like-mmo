{
  "name": "rouge-like-mmo-web",
  "version": "0.0.1",
  "description": "Rouge-like MMO web",
  "scripts": {
    "ng": "./node_modules/@angular/cli/bin/ng",
    "start": "./node_modules/@angular/cli/bin/ng serve --watch --poll 1000",
    "build": "./node_modules/@angular/cli/bin/ng build",
    "watch": "./node_modules/@angular/cli/bin/ng build --watch --poll 1000",
    "test": "./node_modules/@angular/cli/bin/ng test",
    "lint": "./node_modules/@angular/cli/bin/ng lint",
    "e2e": "./node_modules/@angular/cli/bin/ng e2e",
    "doc": "npm run clean-doc && ./node_modules/typedoc/bin/typedoc ./src --exclude /**/*.spec.ts --ignoreCompilerErrors --out ./doc --mode file",
    "clean": "npm run clean-app & npm run clean-doc",
    "clean-app": "rm -rf ./dist",
    "clean-doc": "rm -rf ./doc"
  },
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@angular/common": "^6.1.1",
    "@angular/compiler": "^6.1.1",
    "@angular/core": "^6.1.1",
    "@angular/forms": "^6.1.1",
    "@angular/platform-browser": "^6.1.1",
    "@angular/platform-browser-dynamic": "^6.1.1",
    "@angular/router": "^6.1.1",
    "@ngx-translate/core": "^10.0.2",
    "@ngx-translate/http-loader": "^3.0.1",
    "@types/lodash": "^4.14.117",
    "bootstrap": "3.3.7",
    "core-js": "^2.5.4",
    "lodash": "^4.17.11",
    "ngx-bootstrap": "^3.0.1",
    "rxjs": "^6.0.0",
    "rxjs-compat": "^6.3.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular/cli": "^6.1.2",
    "@angular/compiler-cli": "^6.1.1",
    "@angular/language-service": "^6.1.1",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.2.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.3.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typedoc": "^0.12.0",
    "typescript": "~2.9.2"
  }
}
