{
  "name": "rouge-like-mmo-web",
  "version": "0.0.1",
  "description": "Rouge-like MMO web",
  "scripts": {
    "ng": "./node_modules/@angular/cli/bin/ng",
    "start": "./node_modules/@angular/cli/bin/ng serve --watch --poll 1000",
    "build": "./node_modules/@angular/cli/bin/ng build",
    "watch": "./node_modules/@angular/cli/bin/ng build --watch --poll 1000",
    "test": "./node_modules/@angular/cli/bin/ng test",
    "lint": "./node_modules/@angular/cli/bin/ng lint",
    "e2e": "./node_modules/@angular/cli/bin/ng e2e",
    "doc": "npm run clean-doc && ./node_modules/typedoc/bin/typedoc ./src --exclude /**/*.spec.ts --ignoreCompilerErrors --out ./doc --mode file",
    "clean": "npm run clean-app & npm run clean-doc",
    "clean-app": "rm -rf ./dist",
    "clean-doc": "rm -rf ./doc"
  },
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@angular/common": "^7.1.4",
    "@angular/compiler": "^7.1.4",
    "@angular/core": "^7.1.4",
    "@angular/forms": "^7.1.4",
    "@angular/platform-browser": "^7.1.4",
    "@angular/platform-browser-dynamic": "^7.1.4",
    "@angular/router": "^7.1.4",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@types/lodash": "^4.14.117",
    "bootstrap": "^3.3.7",
    "core-js": "^2.5.4",
    "lodash": "^4.17.11",
    "ngx-bootstrap": "^3.0.1",
    "rxjs": "^6.0.0",
    "rxjs-compat": "^6.3.1",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.4",
    "@angular/cli": "^7.1.4",
    "@angular/compiler-cli": "^7.1.4",
    "@angular/language-service": "^7.1.4",
    "@types/jasmine": "~3.3.4",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~10.12.18",
    "codelyzer": "~4.5.0",
    "jasmine-core": "~3.3.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.4",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.1",
    "ts-node": "~7.0.1",
    "tslint": "~5.12.0",
    "typedoc": "^0.13.0",
    "typescript": "~3.1.6"
  }
}
