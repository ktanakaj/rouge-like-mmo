{
  "name": "rouge-like-mmo-web",
  "version": "0.0.1",
  "description": "Rouge-like MMO web",
  "scripts": {
    "ng": "./node_modules/@angular/cli/bin/ng",
    "start": "./node_modules/@angular/cli/bin/ng serve --watch --poll 1000",
    "build": "./node_modules/@angular/cli/bin/ng build",
    "watch": "./node_modules/@angular/cli/bin/ng build --watch --poll 1000",
    "test": "./node_modules/@angular/cli/bin/ng test",
    "lint": "./node_modules/@angular/cli/bin/ng lint",
    "e2e": "./node_modules/@angular/cli/bin/ng e2e",
    "doc": "npm run clean-doc && ./node_modules/typedoc/bin/typedoc ./src --exclude /**/*.spec.ts --ignoreCompilerErrors --out ./doc --mode file",
    "clean": "npm run clean-app & npm run clean-doc",
    "clean-app": "rm -rf ./dist",
    "clean-doc": "rm -rf ./doc"
  },
  "license": "MIT",
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.15",
    "@angular/common": "^7.2.12",
    "@angular/compiler": "^7.2.12",
    "@angular/core": "^7.2.12",
    "@angular/forms": "^7.2.12",
    "@angular/platform-browser": "^7.2.12",
    "@angular/platform-browser-dynamic": "^7.2.12",
    "@angular/router": "^7.2.12",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@types/lodash": "^4.14.123",
    "bootstrap": "^4.3.1",
    "core-js": "^2.6.5",
    "jquery": "^3.3.1",
    "lodash": "^4.17.11",
    "ngx-bootstrap": "^4.0.0",
    "rxjs": "^6.4.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.8",
    "@angular/cli": "^7.3.8",
    "@angular/compiler-cli": "^7.2.12",
    "@angular/language-service": "^7.2.12",
    "@types/jasmine": "~3.3.12",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~11.13.0",
    "codelyzer": "~5.0.0",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.1.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.5",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.0",
    "protractor": "~5.4.2",
    "ts-node": "^8.1.0",
    "tslint": "^5.16.0",
    "typedoc": "^0.14.2",
    "typescript": ">=3.2.0 <3.3.0"
  }
}
