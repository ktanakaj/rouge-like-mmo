{
  "name": "rouge-like-mmo-svr",
  "version": "0.0.1",
  "description": "Rouge-like MMO server",
  "private": true,
  "scripts": {
    "prebuild": "rm -rf ./dist",
    "build": "./node_modules/@nestjs/cli/bin/nest.js build",
    "watch": "npm run prebuild && ./node_modules/@nestjs/cli/bin/nest.js build --watch",
    "start": "pm2 start process.json",
    "start:prod": "pm2 start process.json --env production",
    "restart": "pm2 restart process.json",
    "stop": "pm2 stop process.json",
    "master-import": "node ./dist/batch/master-import.js",
    "db-migrate": "node ./dist/batch/db-migrate.js",
    "lint": "./node_modules/tslint/bin/tslint -p tsconfig.json -c tslint.json",
    "test": "NODE_ENV=test ./node_modules/jest/bin/jest.js --forceExit",
    "test:watch": "NODE_ENV=test ./node_modules/jest/bin/jest.js --watch",
    "test:cov": "NODE_ENV=test ./node_modules/jest/bin/jest.js --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/@nestjs/cli/bin/nest.js --runInBand",
    "test:e2e": "NODE_ENV=test ./node_modules/jest/bin/jest.js --config ./test/jest-e2e.json --forceExit",
    "predoc": "rm -rf ./doc",
    "doc": "./node_modules/typedoc/bin/typedoc ./src --out ./doc --mode file --ignoreCompilerErrors",
    "clean": "npm run prebuild & npm run predoc"
  },
  "license": "MIT",
  "engines": {
    "node": ">=10.9.0"
  },
  "directories": {
    "test": "src"
  },
  "dependencies": {
    "@nestjs/common": "^6.10.4",
    "@nestjs/core": "^6.10.4",
    "@nestjs/microservices": "^6.10.4",
    "@nestjs/platform-express": "^6.10.4",
    "@nestjs/swagger": "^4.0.0-next.18",
    "@types/bluebird": "^3.5.29",
    "@types/config": "^0.0.36",
    "@types/connect-redis": "^0.0.11",
    "@types/express-session": "^1.15.16",
    "@types/lodash": "^4.14.149",
    "@types/minimist": "^1.2.0",
    "@types/redis": "^2.8.14",
    "@types/validator": "^10.11.3",
    "@types/ws": "^6.0.4",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "config": "^3.2.4",
    "connect-redis": "^4.0.3",
    "csvtojson": "^2.0.10",
    "express-session": "^1.17.0",
    "js-yaml": "^3.13.1",
    "json-rpc2-implementer": "^0.4.0",
    "lodash": "^4.17.15",
    "log4js": "^6.1.0",
    "minimist": "^1.2.0",
    "mysql2": "^2.0.1",
    "random-js": "^2.1.0",
    "redis": "^2.8.0",
    "redis-promisify": "^0.1.1",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.5.3",
    "sequelize": "5.21.2",
    "sequelize-cli": "^5.5.1",
    "sequelize-typescript": "^1.0.0",
    "swagger-ui-express": "^4.1.2",
    "ws": "^7.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.12.6",
    "@nestjs/schematics": "^6.7.4",
    "@nestjs/testing": "^6.10.4",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.14",
    "@types/supertest": "^2.0.8",
    "jest": "^24.9.0",
    "sqlite3": "^4.1.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.2.0",
    "ts-node": "^8.5.4",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.1",
    "typedoc": "^0.15.3",
    "typescript": "^3.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "globalSetup": "./test.js"
  }
}
