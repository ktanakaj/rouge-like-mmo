{
  "name": "rouge-like-mmo-svr",
  "version": "0.0.1",
  "description": "Rouge-like MMO server",
  "private": true,
  "scripts": {
    "prebuild": "rm -rf ./dist",
    "build": "./node_modules/@nestjs/cli/bin/nest.js build",
    "watch": "npm run prebuild && ./node_modules/@nestjs/cli/bin/nest.js build --watch",
    "start": "pm2 start process.json",
    "start:prod": "pm2 start process.json --env production",
    "restart": "pm2 restart process.json",
    "stop": "pm2 stop process.json",
    "master-import": "node ./dist/batch/master-import.js",
    "db-migrate": "node ./dist/batch/db-migrate.js",
    "lint": "./node_modules/tslint/bin/tslint -p tsconfig.json -c tslint.json",
    "test": "NODE_ENV=test ./node_modules/jest/bin/jest.js --forceExit",
    "test:watch": "NODE_ENV=test ./node_modules/jest/bin/jest.js --watch",
    "test:cov": "NODE_ENV=test ./node_modules/jest/bin/jest.js --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/@nestjs/cli/bin/nest.js --runInBand",
    "test:e2e": "NODE_ENV=test ./node_modules/jest/bin/jest.js --config ./test/jest-e2e.json",
    "fulltest": "NODE_ENV=test npm run db-migrate -- --db=master && NODE_ENV=test npm run master-import -- ./test/masters --publish && TARGET=all npm test",
    "predoc": "rm -rf ./doc",
    "doc": "./node_modules/typedoc/bin/typedoc ./src --out ./doc --mode file --ignoreCompilerErrors",
    "clean": "npm run prebuild & npm run predoc"
  },
  "license": "MIT",
  "engines": {
    "node": ">=10.9.0"
  },
  "directories": {
    "test": "src"
  },
  "dependencies": {
    "@nestjs/common": "^6.8.5",
    "@nestjs/core": "^6.8.5",
    "@nestjs/microservices": "^6.8.5",
    "@nestjs/platform-express": "^6.8.5",
    "@nestjs/swagger": "^3.1.0",
    "@types/bluebird": "^3.5.28",
    "@types/config": "^0.0.35",
    "@types/connect-redis": "^0.0.11",
    "@types/express-session": "^1.15.14",
    "@types/lodash": "^4.14.144",
    "@types/minimist": "^1.2.0",
    "@types/random-js": "^1.0.31",
    "@types/redis": "^2.8.14",
    "@types/validator": "^10.11.3",
    "@types/ws": "^6.0.3",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.10.2",
    "config": "^3.2.4",
    "connect-redis": "^4.0.3",
    "csvtojson": "^2.0.10",
    "express-session": "^1.17.0",
    "js-yaml": "^3.13.1",
    "json-rpc2-implementer": "^0.4.0",
    "lodash": "^4.17.15",
    "log4js": "^5.3.0",
    "minimist": "^1.2.0",
    "mysql2": "^2.0.0",
    "random-js": "^2.1.0",
    "redis": "^2.8.0",
    "redis-promisify": "^0.1.1",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.5.3",
    "sequelize": "5.21.1",
    "sequelize-cli": "^5.5.1",
    "sequelize-typescript": "^1.0.0",
    "swagger-ui-express": "^4.1.2",
    "ws": "^7.2.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.11.1",
    "@nestjs/schematics": "^6.7.0",
    "@nestjs/testing": "^6.8.5",
    "@types/express": "^4.17.1",
    "@types/jest": "^24.0.20",
    "@types/node": "^12.11.7",
    "@types/supertest": "^2.0.8",
    "jest": "^24.9.0",
    "sqlite3": "^4.1.0",
    "supertest": "^4.0.2",
    "ts-jest": "^24.1.0",
    "ts-node": "^8.4.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^5.20.0",
    "typedoc": "^0.15.0",
    "typescript": "^3.6.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "globalSetup": "./test.js"
  }
}
