{
  "name": "rouge-like-mmo-svr",
  "version": "0.0.1",
  "description": "Rouge-like MMO server",
  "private": true,
  "scripts": {
    "start": "pm2 start process.json",
    "production": "pm2 start process.json --env production",
    "restart": "pm2 restart process.json",
    "stop": "pm2 stop process.json",
    "master-import": "node ./dist/batch/master-import.js",
    "db-migrate": "node ./dist/batch/db-migrate.js",
    "build": "npm run clean-app && ./node_modules/typescript/bin/tsc --outDir ./dist",
    "watch": "npm run clean-app && ./node_modules/typescript/bin/tsc --watch --outDir ./dist",
    "test": "NODE_ENV=test ./node_modules/mocha/bin/mocha --require espower-typescript/guess ./src/mocha.ts \"./src/**/*.spec.ts\" --exit",
    "fulltest": "NODE_ENV=test npm run db-migrate -- --db=master && NODE_ENV=test npm run master-import -- ./test --publish && TARGET=all npm test",
    "doc": "npm run clean-doc && ./node_modules/typedoc/bin/typedoc ./src --out ./doc --mode file --ignoreCompilerErrors",
    "clean": "npm run clean-app & npm run clean-doc",
    "clean-app": "rm -rf ./dist",
    "clean-doc": "rm -rf ./doc",
    "lint": "./node_modules/tslint/bin/tslint -p tsconfig.json -c tslint.json"
  },
  "license": "MIT",
  "engines": {
    "node": ">=10.9.0"
  },
  "directories": {
    "test": "src"
  },
  "dependencies": {
    "@nestjs/common": "^6.7.2",
    "@nestjs/core": "^6.7.2",
    "@nestjs/microservices": "^6.7.2",
    "@nestjs/platform-express": "^6.7.2",
    "@nestjs/swagger": "^3.1.0",
    "@types/bluebird": "^3.5.26",
    "@types/config": "^0.0.34",
    "@types/connect-redis": "^0.0.11",
    "@types/express-session": "^1.15.14",
    "@types/lodash": "^4.14.138",
    "@types/minimist": "^1.2.0",
    "@types/random-js": "^1.0.31",
    "@types/redis": "^2.8.13",
    "@types/sequelize": "^4.27.25",
    "@types/ws": "^6.0.3",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.10.0",
    "config": "^3.2.2",
    "connect-redis": "^4.0.2",
    "csvtojson": "^2.0.10",
    "express-session": "^1.16.2",
    "js-yaml": "^3.13.1",
    "json-rpc2-implementer": "^0.4.0",
    "lodash": "^4.17.15",
    "log4js": "^5.1.0",
    "minimist": "^1.2.0",
    "mysql2": "^1.6.1",
    "random-js": "^2.1.0",
    "redis": "^2.8.0",
    "redis-promisify": "^0.1.1",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.5.3",
    "sequelize": "4.40.0",
    "sequelize-cli": "^5.4.0",
    "sequelize-typescript": "^0.6.9",
    "swagger-ui-express": "^4.1.1",
    "typescript": "^3.6.3",
    "ws": "^7.1.2"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.7.2",
    "@types/express": "^4.17.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.5",
    "@types/power-assert": "^1.5.0",
    "espower-typescript": "^9.0.1",
    "mocha": "^6.2.0",
    "power-assert": "^1.6.1",
    "sqlite3": "^4.1.0",
    "tslint": "^5.20.0",
    "typedoc": "^0.15.0"
  }
}
